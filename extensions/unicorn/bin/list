#!/usr/bin/env bash

source $extension_scripts_path/initialize

if [[ "$master_pid" -le 0 ]] ; then
  $scripts_path/log "info" "There are no unicorns running for $project."
else
  ps auxww | \grep '[u]nicorn' | \grep master
  echo
  ps auxwww | \grep '[u]nicorn' | \grep $project | \grep $environment | \grep worker
fi

exit $?

