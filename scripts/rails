#!/usr/bin/env bash

#
# Rails
#
migrate() {
  builtin cd $current_path
  rake db:migrate --trace
}

console() {
  builtin cd $current_path
  if [[ -f script/console ]] ; then ruby script/console ; fi
}

dbconsole() {
  builtin cd $current_path
  if [[ -f script/console ]] ; then ruby script/dbconsole ; fi
}

bootstrap() {
  # rake gems:install
  # rvm gems load
  # rake db:create
  # rake db:migrate
}
